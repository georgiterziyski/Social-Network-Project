type User {
	id		 : ID!
	userName : String!
	email	 : String!
}

type Query {
    allUsers : [User]
 #   getUser(email : String) : User

}

input UserInput {
	userName : String!
	email	 : String!
	password : String!
}

type Mutation {
	addUser(input : UserInput!) : User
}

schema {
    query    : Query
   # mutation : Mutation
}